# vim: ft=sh ts=4 sts=4 sw=4 et
# Path to the bash it configuration
export BASH_IT="${HOME}/.bash_it"

# Lock and Load a custom theme file
# location /.bash_it/themes/
export BASH_IT_THEME='doubletime'

# Load Bash It
. $BASH_IT/bash_it.sh

. ~/.shell/interactive

# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/home/spauka/anaconda3/bin/conda' 'shell.bash' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/home/spauka/anaconda3/etc/profile.d/conda.sh" ]; then
        . "/home/spauka/anaconda3/etc/profile.d/conda.sh"
    else
        export PATH="/home/spauka/anaconda3/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda initialize <<<

